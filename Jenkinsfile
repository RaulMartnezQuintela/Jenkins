pipeline
{
    agent any
    stages
    {
        stage("Salto de linea")
        {
            steps
            {
                script
                {
                def contenido = "Hola que tal este es mi contenido\n" +
                                "El contenido sigue aqui abajo"
                println contenido    
                }
                
            }
            
        }
    
    }
    
}
